<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:vm="urn:import:ro.pub.cs.vmchecker.client.ui">
	<ui:style>
		.content {
			width: 700px;
		}

		.contentWrapper {
			width: 100%;
		}

		.contentPanel {
		}

		.statsButton {
			margin-left: 22px;
		}
	</ui:style>
	<g:SimplePanel styleName="{style.contentWrapper}">
		<g:HorizontalPanel styleName="{style.contentPanel}">
			<g:FlowPanel>
				<g:Anchor ui:field="viewStatsButton" styleName="{style.statsButton} fillButton">rezultate generale</g:Anchor>
				<g:SimplePanel ui:field="menuPanel">
				</g:SimplePanel>
			</g:FlowPanel>
			<g:SimplePanel ui:field="boardPanel" styleName="{style.content}">
			</g:SimplePanel>
			<g:VerticalPanel ui:field="sidePanel">
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:SimplePanel>
</ui:UiBinder>
